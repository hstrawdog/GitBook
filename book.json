{
    "title": "gitbook tutorial",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [ 
        "-search",
        "github",
        "hide-element",
        "splitter", 
        "tbfed-pagefooter",
        "back-to-top-button",
        "code",
        "page-toc-button" 
    ],
    "pluginsConfig": {
        "code": {
        "copyButtons": true
        },
          "hide-element": {
            "elements": [".gitbook-link"]
        },
        "github": {
            "url": "https://github.com/huangqiqiang/Book"
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy huangqiqiang 2020",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        }
    }
}
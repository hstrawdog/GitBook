{
    "title": "gitbook tutorial",
    "language": "zh-hans",
    "structure": {
        "readme": "README.md"
    },
    "styles": {
        "website": "styles/website.css"
    },
    "theme-default": {
        "showLevel": true
    },
    "plugins": [
        "-search",
        "-lunr",
        "-highlight",
        "github",
        "hide-element",
        "splitter",
        "tbfed-pagefooter",
        "code",
        "chapter-fold",
        "page-treeview",
        "anchor-navigation-ex",
        "popup",
        "pageview-count"
    ],
    "pluginsConfig": {
        "page-treeview": {
            "copyright": "Copyright &#169; aleen42",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        },
        "code": {
            "copyButtons": true
        },
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "github": {
            "url": "https://github.com/huangqiqiang/Book"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy qiqiang 2020",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "theme-default": {
            "showLevel": false,
            "mode": "float",
            "float": {
                "floatIcon": "fa fa-navicon",
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        }
    }
}